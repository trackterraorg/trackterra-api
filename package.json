  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npx nest build",
    "format": "prettier --write \"server/**/*.ts\" \"libs/**/*.ts\"",
    "start": "npx nest start",
    "start:dev": "npx nest start --watch",
    "start:debug": "npx nest start --debug --watch",
    "start:prod": "node dist/server/gateway-admin/main.js",
    "setup:local": "sh gen-ts.sh && nest build proto-schema && sh scripts/register.sh",
    "setup:proto": "sh gen-ts.sh && nest build proto-schema",
    "setup:consul": "sh scripts/register.sh",
    "lint": "eslint",
    "lint-md": "remark .",
    "test": "env MONGOMS_ARCH=x64 jest",
    "test:watch": "env MONGOMS_ARCH=x64 jest --watch",
    "test:cov": "env MONGOMS_ARCH=x64 jest --coverage && cat ./coverage/lcov.info | codacy-coverage --token a5ea7c309f7543a3b7cfc662c1c88d67 --language typescript",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env MONGOMS_ARCH=x64 jest --config ./server/gateway-admin/server/signals/dfour-microservice-backend/test/jest-e2e.json",
    "docs": "npx compodoc --theme material --output docs -p tsconfig.json -s",
    "prepare": ""
  },
