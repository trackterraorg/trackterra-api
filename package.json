  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "preset": "@shelf/jest-mongodb",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "modulePaths": [
      "<rootDir>/server/",
      "<rootDir>/libs/"
    ],
    "roots": [
      "<rootDir>/server/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@trackterra/common/(.*)": "<rootDir>/libs/common/src/$1",
      "@trackterra/common": "<rootDir>/libs/common/src",
      "@trackterra/contracts/(.*)": "<rootDir>/libs/contracts/src/$1",
      "@trackterra/contracts": "<rootDir>/libs/contracts/src",
      "@trackterra/core/(.*)": "<rootDir>/libs/core/src/$1",
      "@trackterra/core": "<rootDir>/libs/core/src",
      "@trackterra/parser/(.*)": "<rootDir>/libs/parser/src/$1",
      "@trackterra/parser": "<rootDir>/libs/parser/src",
      "@trackterra/proto-schema/(.*)": "<rootDir>/libs/proto-schema/src/$1",
      "@trackterra/proto-schema": "<rootDir>/libs/proto-schema/src",
      "@trackterra/repository/(.*)": "<rootDir>/libs/repository/src/$1",
      "@trackterra/repository": "<rootDir>/libs/repository/src",
      "@juicycleff/repo-orm/(.*)": "<rootDir>/libs/repo-orm/src/$1",
      "@juicycleff/repo-orm": "<rootDir>/libs/repo-orm/src"
    }
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format"
    }
  }
